   "source": [
    "mtcars %>%  rownames_to_column(\"model\") %>% \n",
    "            mutate(cyl = as.character(cyl)) %>% \n",
    "            filter(cyl == 4) %>% \n",
    "            select(model,wt,cyl,mpg) %>%\n",
    "            mutate(CPW = mpg/wt) %>%\n",
    "            arrange(desc(mpg))"
   ]
